---
layout: nil
---

<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    <url>
        <loc>{{ site.url }}</loc>
        <priority>1.000</priority>
    </url>
    {% for post in site.posts %}
    <url>
        <loc>{{ site.url }}{{ post.url }}</loc>
        <priority>0.500</priority>
    </url>
    {% endfor %}
</urlset>
